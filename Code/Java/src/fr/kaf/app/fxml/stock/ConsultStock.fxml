<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TreeTableColumn?>
<?import javafx.scene.control.TreeTableView?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="ConsultStock" maxHeight="300.0" maxWidth="400.0" minHeight="300.0" minWidth="400.0" prefHeight="300.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fr.kaf.app.controller.stock.ConsultStockController">
   <children>
      <StackPane maxHeight="50.0" minHeight="50.0" prefHeight="50.0" prefWidth="200.0">
         <VBox.margin>
            <Insets bottom="10.0" left="15.0" right="15.0" top="15.0" />
         </VBox.margin>
         <children>
            <Label text="Emplacement :" StackPane.alignment="CENTER_LEFT" />
            <ComboBox prefWidth="150.0" />
         </children>
      </StackPane>
      <TreeTableView prefHeight="300.0" prefWidth="200.0">
        <columns>
          <TreeTableColumn prefWidth="215.0" text="Référence" />
          <TreeTableColumn prefWidth="154.0" text="Quantité" />
        </columns>
         <VBox.margin>
            <Insets bottom="5.0" left="15.0" right="15.0" />
         </VBox.margin>
      </TreeTableView>
      <StackPane prefHeight="150.0" prefWidth="200.0">
         <VBox.margin>
            <Insets bottom="5.0" left="15.0" right="15.0" top="5.0" />
         </VBox.margin>
         <children>
            <Button mnemonicParsing="false" text="Retour au menu" onAction="#goStockAction" StackPane.alignment="CENTER_RIGHT" />
         </children>
      </StackPane>
   </children>
</VBox>
