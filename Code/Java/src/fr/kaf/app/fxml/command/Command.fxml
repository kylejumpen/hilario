<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="Command" maxHeight="90.0" maxWidth="300.0" minHeight="90.0" minWidth="300.0" prefHeight="90.0" prefWidth="250.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fr.kaf.app.controller.command.CommandController">
   <children>
      <ButtonBar buttonMinWidth="125.0" prefHeight="40.0" prefWidth="200.0">
        <buttons>
            <Button maxHeight="25.0" maxWidth="100.0" minHeight="25.0" minWidth="100.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="100.0" onAction="#goUnitAction" text="Unité">
               <padding>
                  <Insets left="10.0" right="10.0" />
               </padding>
            </Button>
            <Button maxHeight="25.0" maxWidth="100.0" minHeight="25.0" minWidth="100.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="100.0" onAction="#goBulkAction" text="En gros" />
        </buttons>
         <VBox.margin>
            <Insets left="10.0" right="10.0" />
         </VBox.margin>
      </ButtonBar>
      <ButtonBar buttonMinWidth="125.0" prefHeight="40.0" prefWidth="200.0">
        <buttons>
            <Button mnemonicParsing="false" text="Revenir à l'accueil" onAction="#goHomeAction"/>
            <Button maxHeight="25.0" maxWidth="85.0" minHeight="25.0" minWidth="85.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="85.0" onAction="#closeButtonAction" text="Quitter" />
        </buttons>
         <VBox.margin>
            <Insets left="10.0" right="10.0" />
         </VBox.margin>
      </ButtonBar>
   </children>
</VBox>
