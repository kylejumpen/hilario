<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="Unit" maxHeight="275.0" maxWidth="300.0" minHeight="275.0" minWidth="300.0" prefHeight="275.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fr.kaf.app.controller.command.UnitController">
   <children>
      <StackPane maxHeight="35.0" minHeight="35.0" prefHeight="35.0">
         <children>
            <Label alignment="TOP_LEFT" maxHeight="20.0" maxWidth="70.0" minHeight="20.0" minWidth="70.0" prefHeight="20.0" prefWidth="70.0" text="Référence :" StackPane.alignment="CENTER_LEFT">
               <StackPane.margin>
                  <Insets left="15.0" />
               </StackPane.margin>
            </Label>
            <ComboBox maxHeight="25.0" maxWidth="100.0" minHeight="25.0" minWidth="100.0" prefHeight="25.0" prefWidth="100.0" StackPane.alignment="CENTER_RIGHT">
               <StackPane.margin>
                  <Insets right="15.0" />
               </StackPane.margin>
            </ComboBox>
         </children>
         <VBox.margin>
            <Insets bottom="10.0" top="30.0" />
         </VBox.margin>
      </StackPane>
      <StackPane maxHeight="35.0" minHeight="35.0" prefHeight="35.0">
         <children>
            <Label alignment="TOP_LEFT" maxHeight="20.0" maxWidth="70.0" minHeight="20.0" minWidth="70.0" prefHeight="20.0" prefWidth="70.0" text="Quantité :" StackPane.alignment="CENTER_LEFT">
               <StackPane.margin>
                  <Insets left="15.0" />
               </StackPane.margin>
            </Label>
            <TextField fx:id="quantityFld" maxHeight="25.0" maxWidth="50.0" minHeight="25.0" minWidth="50.0" prefHeight="25.0" prefWidth="50.0" StackPane.alignment="CENTER_RIGHT">
               <StackPane.margin>
                  <Insets right="15.0" />
               </StackPane.margin></TextField>
         </children>
         <VBox.margin>
            <Insets bottom="10.0" />
         </VBox.margin>
      </StackPane>
      <StackPane maxHeight="35.0" minHeight="35.0" prefHeight="35.0">
         <children>
            <Label alignment="TOP_LEFT" maxHeight="20.0" maxWidth="80.0" minHeight="20.0" minWidth="70.0" prefHeight="20.0" prefWidth="70.0" text="Prix de vente :" StackPane.alignment="CENTER_LEFT">
               <StackPane.margin>
                  <Insets left="15.0" />
               </StackPane.margin>
            </Label>
            <TextField fx:id="priceFld" maxHeight="25.0" maxWidth="50.0" minHeight="25.0" minWidth="50.0" prefHeight="25.0" prefWidth="50.0" StackPane.alignment="CENTER_RIGHT">
               <StackPane.margin>
                  <Insets right="15.0" />
               </StackPane.margin></TextField>
         </children>
         <VBox.margin>
            <Insets bottom="15.0" />
         </VBox.margin>
      </StackPane>
      <StackPane maxHeight="35.0" minHeight="35.0" prefHeight="35.0">
         <children>
            <Label alignment="CENTER" maxHeight="25.0" maxWidth="70.0" minHeight="25.0" minWidth="70.0" prefHeight="25.0" prefWidth="70.0" text="Estimation :" StackPane.alignment="CENTER_LEFT">
               <StackPane.margin>
                  <Insets left="15.0" />
               </StackPane.margin>
            </Label>
            <Label fx:id="estimationLbl" alignment="CENTER" maxHeight="25.0" maxWidth="50.0" minHeight="25.0" minWidth="50.0" prefHeight="25.0" prefWidth="50.0" text="Prix" StackPane.alignment="CENTER_RIGHT">
               <StackPane.margin>
                  <Insets right="15.0" />
               </StackPane.margin></Label>
         </children>
         <VBox.margin>
            <Insets bottom="15.0" />
         </VBox.margin>
      </StackPane>
      <ButtonBar prefHeight="40.0" prefWidth="200.0">
        <buttons>
            <Button mnemonicParsing="false" onAction="#goCommandAction" text="Commande" />
            <Button fx:id="cancelButton" cancelButton="true" mnemonicParsing="false" onAction="#cancelButtonAction" text="Remise à Zéro" />
            <Button mnemonicParsing="false" text="Confirmer" />
        </buttons>
         <padding>
            <Insets right="15.0" />
         </padding>
      </ButtonBar>
   </children>
</VBox>
