<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TreeTableColumn?>
<?import javafx.scene.control.TreeTableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<HBox fx:id="Supply" maxWidth="750.0" minWidth="750.0" prefWidth="750.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fr.kaf.app.controller.accounting.SupplyController">
   <children>
      <VBox maxHeight="300.0" maxWidth="400.0" minHeight="300.0" minWidth="400.0">
         <children>
            <StackPane maxHeight="50.0" minHeight="50.0" prefHeight="50.0">
               <VBox.margin>
                  <Insets left="15.0" right="15.0" top="5.0" />
               </VBox.margin>
               <children>
                  <Label text="Mois :" StackPane.alignment="CENTER_LEFT">
                     <font>
                        <Font size="15.0" />
                     </font>
                  </Label>
                  <ComboBox prefWidth="100.0" StackPane.alignment="CENTER_RIGHT" />
               </children>
            </StackPane>
            <StackPane maxHeight="50.0" minHeight="50.0" prefHeight="50.0">
               <VBox.margin>
                  <Insets left="15.0" right="15.0" />
               </VBox.margin>
               <children>
                  <Label text="Année :" StackPane.alignment="CENTER_LEFT">
                     <font>
                        <Font size="15.0" />
                     </font>
                  </Label>
                  <ComboBox prefWidth="100.0" StackPane.alignment="CENTER_RIGHT" />
               </children>
            </StackPane>
            <TreeTableView prefHeight="200.0" prefWidth="200.0">
              <columns>
                <TreeTableColumn prefWidth="284.0" text="Fournisseur" />
                <TreeTableColumn prefWidth="85.0" text="Montant" />
              </columns>
               <VBox.margin>
                  <Insets left="15.0" right="15.0" />
               </VBox.margin>
            </TreeTableView>
            <StackPane maxHeight="50.0" minHeight="50.0" prefHeight="50.0">
               <VBox.margin>
                  <Insets left="15.0" right="15.0" top="5.0" />
               </VBox.margin>
               <children>
                  <Button mnemonicParsing="false" text="Supprimer" StackPane.alignment="CENTER_LEFT" />
                  <Button mnemonicParsing="false" onAction="#goAccounting" text="Retour au menu" StackPane.alignment="CENTER_RIGHT" />
               </children>
            </StackPane>
         </children>
      </VBox>
      <VBox prefHeight="300.0" prefWidth="371.0">
         <children>
            <Label contentDisplay="CENTER" text="Détails" textAlignment="CENTER" underline="true">
               <VBox.margin>
                  <Insets bottom="15.0" left="150.0" right="150.0" top="15.0" />
               </VBox.margin>
               <font>
                  <Font name="System Bold" size="15.0" />
               </font>
            </Label>
            <StackPane prefHeight="50.0" prefWidth="200.0">
               <children>
                  <Label text="Référence" StackPane.alignment="CENTER_LEFT">
                     <StackPane.margin>
                        <Insets left="15.0" />
                     </StackPane.margin>
                  </Label>
                  <ComboBox prefWidth="100.0" StackPane.alignment="CENTER_RIGHT">
                     <StackPane.margin>
                        <Insets right="15.0" />
                     </StackPane.margin>
                  </ComboBox>
               </children>
            </StackPane>
            <StackPane prefHeight="50.0" prefWidth="200.0">
               <VBox.margin>
                  <Insets bottom="5.0" top="5.0" />
               </VBox.margin>
               <children>
                  <Label text="Quantité" StackPane.alignment="CENTER_LEFT">
                     <StackPane.margin>
                        <Insets left="15.0" />
                     </StackPane.margin>
                  </Label>
                  <Label text="Label" StackPane.alignment="CENTER_RIGHT">
                     <StackPane.margin>
                        <Insets right="15.0" />
                     </StackPane.margin>
                  </Label>
               </children>
            </StackPane>
            <StackPane prefHeight="50.0" prefWidth="200.0">
               <VBox.margin>
                  <Insets bottom="5.0" top="5.0" />
               </VBox.margin>
               <children>
                  <Label text="Prix totale" StackPane.alignment="CENTER_LEFT">
                     <StackPane.margin>
                        <Insets left="15.0" />
                     </StackPane.margin>
                  </Label>
                  <Label text="Label" StackPane.alignment="CENTER_RIGHT">
                     <StackPane.margin>
                        <Insets right="15.0" />
                     </StackPane.margin>
                  </Label>
               </children>
            </StackPane>
            <StackPane prefHeight="50.0" prefWidth="200.0">
               <children>
                  <Label text="Prix Unitaire" StackPane.alignment="CENTER_LEFT">
                     <StackPane.margin>
                        <Insets left="15.0" />
                     </StackPane.margin>
                  </Label>
                  <Label text="Label" StackPane.alignment="CENTER_RIGHT">
                     <StackPane.margin>
                        <Insets right="15.0" />
                     </StackPane.margin>
                  </Label>
               </children>
            </StackPane>
         </children>
      </VBox>
   </children>
</HBox>
